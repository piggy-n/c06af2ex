{
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development umi dev",
    "build": "cross-env NODE_ENV=production umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.41"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.41",
    "ahooks": "^3.7.11",
    "antd": "^5.16.4",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "immer": "^10.0.4",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.21",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "ws-player": "^1.4.6",
    "yorkie": "^2.0.0",
    "zustand": "^4.5.2"
  }
}
